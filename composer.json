{
    "name": "vinkla/wordplate",
    "description": "The WordPlate boilerplate",
    "license": "MIT",
    "type": "project",
    "keywords": [
      "wordplate",
      "wordpress"
    ],
    "require": {
      "php": "^8.2",
      "composer/installers": "^2.2",
      "roots/bedrock-autoloader": "^1.0.4",
      "roots/wordpress": "^6.5.4",
      "roots/wp-password-bcrypt": "^1.1",
      "symfony/http-foundation": "^7.1",
      "vinkla/headache": "^3.3",
      "vlucas/phpdotenv": "^5.6",
      "wpackagist-plugin/clean-image-filenames": "^1.5",
      "wpackagist-plugin/custom-post-type-ui": "^1.13",
      "wpackagist-plugin/duplicate-page": "^4.5",
      "wpackagist-plugin/simple-custom-post-order": "^2.5",
      "wpackagist-plugin/speculation-rules": "1.3.1",
      "wpackagist-plugin/svg-support": "2.5.5"
    },
    "require-dev": {
      "symfony/var-dumper": "^7.1"
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://wpackagist.org",
        "only": [
          "wpackagist-plugin/*",
          "wpackagist-theme/*"
        ]
      }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
      "files": [
        "src/helpers.php"
      ]
    },
    "config": {
      "allow-plugins": {
        "composer/installers": true,
        "roots/wordpress-core-installer": true
      },
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
    },
    "extra": {
      "installer-paths": {
        "public_html/mu-plugins/{$name}": [
          "type:wordpress-muplugin",
          "wpackagist-plugin/clean-image-filenames"
        ],
        "public_html/plugins/{$name}": [
          "type:wordpress-plugin"
        ],
        "public_html/themes/{$name}": [
          "type:wordpress-theme"
        ]
      },
      "wordpress-install-dir": "public_html/wordpress"
    },
    "scripts": {
      "post-root-package-install": [
        "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
      ]
    }
  }
  